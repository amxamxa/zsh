: 1712748813:0;matrix_col() {\
    local lines=$(tput lines)\
    cols=$(tput cols)\
\
    # AWK-Skript f√ºr die Matrix-Animation mit Farben\
    awkscript='\
    {      letters="FUCKfuck"\
     	   lines=$1\
        	random_col=$3\
        	c=$4\
        	letter=substr(letters,c,1)\
        	cols[random_col]=0;\
        	for (col in cols) {\
            	line=cols[col];\
            	cols[col]=cols[col]+1;\
            	# Zuf√§llige Farbauswahl f√ºr jeden Buchstaben\
            	color=rand() * 6 + 31\
            \
            	# Ausgabe des aktuellen Buchstabens in zuf√§lliger Farbe an der aktuellen Position\
            	printf "\033[%s;%sH\033[38;5;%dm%s", line, col, color, letter;\
            	# Zur√ºcksetzen der Farbe f√ºr das n√§chste Zeichen\
            	printf "\033[%s;%sH\033[0m%s\033[0;0H", cols[col], col, letter;\
            \
            		if (cols[col] >= lines) {\
                	cols[col]=0;\
            								}\
        						}   \
    }\
    '\
\
	    # Hintergrundfarbe des Terminals auf Schwarz √§ndern\
	    echo -e "\e[1;40m"\
	    # Bildschirm l√∂schen\
	    clear\
\
	    # Endlosschleife f√ºr die Matrix-Animation\
	    while :; do\
	        # Generieren von vier zuf√§lligen Werten und Ausgabe an AWK\
	        echo $lines $cols $(( $RANDOM % $cols)) $(( $RANDOM % 72 ))\
	        # Kurze Pause f√ºr die Animation\
	        sleep 0.1\
	    # AWK-Skript auf die Ausgabe anwenden, um die Matrix-Animation zu erzeugen\
    done | awk "$awkscript"\
}
: 1712749141:0;[200~matrix_col() {\
    local lines=$(tput lines)\
    local cols=$(tput cols)\
\
    # AWK-Skript f√ºr die Matrix-Animation mit Farben\
    awkscript='\
    BEGIN {\
        srand();\
        letters[1]="FUCK";\
        letters[2]="fuck";\
        num_letters=2;\
    }\
    {\
        line=$1;\
        col=$2;\
        letter=letters[(line+col) % num_letters + 1];\
        color=int(rand() * 6) + 31;\
        printf "\033[%s;%sH\033[38;5;%dm%s", line, col, color, letter;\
        printf "\033[%s;%sH\033[0m%s\033[0;0H", line+1, col, letter;\
    }\
    '\
\
    # Hintergrundfarbe des Terminals auf Schwarz √§ndern\
    echo -e "\e[1;40m"\
    # Bildschirm l√∂schen\
    clear\
\
    # Endlosschleife f√ºr die Matrix-Animation\
    while :; do\
        # Generieren von zwei zuf√§lligen Werten und Ausgabe an AWK\
        echo $((RANDOM % lines)) $((RANDOM % cols)) | awk "$awkscript"\
        # Kurze Pause f√ºr die Animation\
        sleep 0.02\
    done\
}
