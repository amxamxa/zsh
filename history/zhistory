    local end=$2\
    if [[ $start =~ ^[0-9A-Fa-f]{1,5}$ && $end =~ ^[0-9A-Fa-f]{1,5}$ ]]; then\
        for (( i=16#$start; i<=16#$end; i++ )); do\
            printf "\\U%08x \t" $i\
            printf "%b\n" "\\U$(printf "%08x" $i)"\
        done\
    else\
        echo "Invalid input. Please enter a valid hexadecimal range."\
    fi\
}
: 1715123700:0;\
# Print range function\
print_range() {\
    local start=$1\
    local end=$2\
    # pattern checks for a string that starts with '0x' followed by exactly four hexadecimal \
    if [[ $start =~ ^0x[0-9A-Fa-f]{4}$ && $end =~ ^0x[0-9A-Fa-f]{4}$ ]]; then\
        for ((i=0x$start; i<=0x$end; i++)); do\
            printf "\\u%04x \t" $i\
            printf "%b\n" "\\u$i"\
        done\
    else\
        echo "Ung√ºltige Eingabe. Bitte geben Sie einen g√ºltigen hexadezimalen Bereich ein."\
    fi\
}\
